Include /Users/alessandro/.colima/ssh_config

Include /Users/alessandrror/.colima/ssh_config

# IT GOES account for every IT GOES repository
Host github.com-it-goes
        User alessandro-rivas-dev
        HostName github.com
        IdentityFile ~/.ssh/id_rsa_it_goes

# Personal account
Host github.com
        User alessandrror
        HostName github.com
        IdentityFile ~/.ssh/id_rsa_personal

Host *
        AddKeysToAgent yes
        IdentitiesOnly yes
        UseKeychain yes
        Compression yes
        PreferredAuthentications publickey
